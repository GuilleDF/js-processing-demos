{
  "name": "js-processing-demos",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:GuilleDF/js-processing-demos.git",
  "author": "Guille De Fermin <guillermo.defermin@taxfix.de>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf demos/transpiling/*.out.js demos/typescript/*.js demos/bundling/*/dist",
    "transpiling/basic": "babel --presets=@babel/preset-env demos/transpiling/basic.js -o demos/transpiling/basic.out.js",
    "transpiling/react": "babel --presets=@babel/preset-{env,react} demos/transpiling/react.jsx -o demos/transpiling/react.out.js",
    "typescript/basic": "tsc demos/typescript/basic.ts",
    "bundling/basic": "cd demos/bundling/basic; rollup -c rollup.config.js",
    "bundling/minified": "cd demos/bundling/minified; rollup -c rollup.config.js",
    "bundling/dependencies": "cd demos/bundling/dependencies; rollup -c rollup.config.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "rollup": "^2.26.3",
    "rollup-plugin-terser": "^7.0.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "ramda": "^0.27.1",
    "react": "^16.13.1"
  }
}
